include "system/public/zircon/mdi/zircon.mdi"

kernel = {
    // Keep pll011 uart at top so it is initialized first
    //  (uart6)
    arm-pl011-uart = {
        base-phys = 0xfff32000
        irq = 111
    }
    arm-gic-v2 = {
        base-phys = 0xe82b0000
        gicd-offset = 0x1000
        gicc-offset = 0x2000
        gich-offset = 0x4000
        gicv-offset = 0x6000
        ipi-base = 13
    }
    arm-psci = {
        use-smc = true
    }
    arm-timer = {
        irq-virt = 27
        irq-phys = 30
    }

    hisi-power = {
        sctrl-base-phys = 0xfff0a000
        pmu-base-phys = 0xfff34000
    }
}
